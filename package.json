{
  "name": "brace2",
  "version": "0.12.1",
  "description": "browserify compatible version of the ace editor.",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "update": "(cd build && node ./update && node ./update-ts)",
    "test": "browserify test/*.js > test/bundle.js --debug && opener test/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/budiselic/brace2.git"
  },
  "homepage": "https://github.com/budiselic/brace2",
  "devDependencies": {
    "browserify": "~4.2.3",
    "opener": "~1.5.2",
    "shelljs": "~0.8.5",
    "tape": "~4.17.0",
    "uglify-js": "~2.8.29"
  },
  "keywords": [
    "ace",
    "editor",
    "browser",
    "package",
    "bundle",
    "inline",
    "browserify"
  ],
  "author": {
    "name": "Antonio Budiselic",
    "url": "https://github.com/budiselic"
  },
  "license": "MIT",
  "engine": {
    "node": ">=0.6"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/8..latest",
      "firefox/21..latest",
      "firefox/nightly",
      "chrome/26..latest",
      "chrome/canary",
      "opera/12..latest",
      "opera/next",
      "safari/5.1..latest",
      "ipad/6.0..latest",
      "iphone/6.0..latest",
      "android-browser/4.2..latest"
    ]
  }
}
